        <replacement>documentId</replacement>
      </pattern>
      <pattern>
        <match>toDate</match>
        <replacement>toDateValue</replacement>
      </pattern>
      <pattern>
        <match>fromDate</match>
        <replacement>fromDateValue</replacement>
      </pattern>
      <pattern>
        <match>activeInd</match>
        <replacement>active</replacement>
      </pattern>
      <pattern>
